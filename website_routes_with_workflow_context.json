  "workflow_interruption_guidelines": {
    "interruption_levels": {
      "low": {
        "description": "Safe to navigate, no risk of data loss",
        "action": "Route immediately",
        "examples": ["dashboards", "content browsing", "completed sessions"]
      },
      "medium": {
        "description": "Some risk of data loss, user should be warned",
        "action": "Show confirmation dialog with save option",
        "examples": ["form editing", "content creation", "draft work"]
      },
      "high": {
        "description": "Significant risk of data loss or workflow disruption",
        "action": "Strong warning with explicit confirmation",
        "examples": ["admin configurations", "critical editing sessions"]
      },
      "critical": {
        "description": "Active session that cannot be recovered",
        "action": "Block navigation unless explicitly confirmed",
        "examples": ["live interviews", "proctored assessments", "timed sessions"]
      }
    },
    "data_loss_risk_levels": {
      "none": "No data will be lost",
      "low": "Minimal data loss, easily recoverable",
      "medium": "Moderate data loss, some work may be lost",
      "high": "Significant data loss, substantial work may be lost",
      "critical": "Complete loss of session data, cannot be recovered"
    },
    "chatbot_decision_logic": {
      "always_route": [
        "interruption_level === 'low'",
        "safe_to_route === true && requires_confirmation === false"
      ],
      "confirm_before_route": [
        "interruption_level === 'medium' || interruption_level === 'high'",
        "requires_confirmation === true"
      ],
      "strongly_discourage": [
        "interruption_level === 'critical'",
        "data_loss_risk === 'critical'",
        "active_session_type in ['live_interview', 'proctored_assessment']"
      ],
      "alternative_actions": [
        "Suggest completing current task first",
        "Offer to bookmark the requested page",
        "Provide information about the requested page without routing",
        "Schedule the navigation for after current session"
      ]
    }
  },
